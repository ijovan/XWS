		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="bankaService">
		<wsdl:port name="bankaPort" binding="tns:bankaBinding">
			<soap:address location="http://localhost:9090/banka" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>
